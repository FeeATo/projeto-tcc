<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white">

    <TextView
        android:id="@+id/textView29"
        android:layout_width="301dp"
        android:layout_height="22dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="30dp"
        android:text="Quantidade de serviços realizados:"
        android:textColor="#FF939393"
        android:textSize="18dp" />

    <TextView
        android:id="@+id/txtServicoCount"
        android:layout_width="72dp"
        android:layout_height="22dp"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="330dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="9dp"

        android:paddingTop="0dp"
        android:text="0"
        android:textColor="@android:color/black"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/textView30"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="90dp"
        android:layout_marginTop="68dp"
        android:text="Avaliação média:"
        android:textColor="#FF939393"
        android:textSize="12dp"

        />

    <RatingBar
        android:id="@+id/ratingBar"
        android:layout_width="243dp"
        android:layout_height="45dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="90dp"
        android:clickable="false"
        android:layout_centerHorizontal="true"
        android:numStars="5"
        android:rating="3.5"
        android:progressBackgroundTint="#dee6ff"
        android:progressTint="#7d9dff"
        android:secondaryProgressTint="#3455b9"
        android:isIndicator="true"
        />

    <TextView
        android:id="@+id/txtPlano"
        android:layout_width="278dp"
        android:layout_height="58dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="160dp"
        android:background="@drawable/menu_plano_nenhum"
        android:fontFamily="@font/baloo"
        android:paddingStart="15dp"
        android:paddingTop="16dp"
        android:text="Nenhum plano contratado"
        android:textColor="#3F51B5"
        android:textSize="16sp"

        />

    <TextView
        android:id="@+id/fundoazul"
        android:layout_width="382dp"
        android:layout_height="468dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="250dp"
        android:background="@drawable/menuembaixoprestador"
        />

    <Button
        android:id="@+id/btnToPlano"
        android:layout_width="294dp"
        android:layout_height="132dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="380dp"
        android:textSize="15dp"
        android:paddingBottom="5dp"
        android:paddingRight="90dp"
        android:textColor="#636363"
        android:background="@drawable/botaocontratar_plano_teste"

        android:text="Contrate o plano
        premium >" />

    <ImageView
        android:id="@+id/imgIcon"
        android:layout_width="23dp"
        android:layout_height="23dp"
        android:layout_marginLeft="300dp"
        android:layout_marginTop="177dp"
        tools:srcCompat="@drawable/semplano_icon"

        />


</RelativeLayout>